<h1>Users</h1>
<% @users.each do |user| %>
  <div>
    <h2><%= link_to user.name, user %></h2>
    <p><%= user.DOB %></p>
    <p><%= user.email %></p>
    <p><%= user.phone_number %></p>
    <p><%= user.address %></p>
    <%= link_to 'Edit', edit_user_path(user) %> |
    <%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
<% end %>
<br>
<%= link_to 'New user', new_user_path %>